{
    "max_open_trades": 1,
    "stake_currency": "USDT",
    "stake_amount": "unlimited",
    "last_stake_amount_min_ratio": 0.0,
    "last_stake_amount_max": 0.0,
    "tradable_balance_ratio": 0.99,
    "amend_last_stake_amount": false,
    "dry_run": true,
    "dry_run_wallet": 1000,
    "cancel_open_orders_on_exit": false,
    "process_only_new_candles": true,
    "minimal_roi": {
        "0": 0.1
    },
    "stoploss": {
        "stoploss_on_exchange": false,
        "stoploss_on_exchange_interval": 60
    },
    "trailing_stoploss": {
        "enabled": false
    },
    "trailing_stoploss_pos_offset": 0.0,
    "trailing_stoploss_offset": 0.0,
    "unfilledtimeout": {
        "buy": 10,
        "sell": 10,
        "exit_timeout_count": 0,
        "unit": "minutes"
    },
    "entry_pricing": {
        "price_side": "same",
        "use_order_book": true,
        "order_book_top": 1,
        "price_last_balance": 0.0,
        "check_depth_of_market": {
            "enabled": false,
            "bids_to_ask_delta": 1
        }
    },
    "exit_pricing": {
        "price_side": "other",
        "use_order_book": true,
        "order_book_top": 1
    },
    "exchange": {
        "name": "binance",
        "key": "${BINANCE_API_KEY}",
        "secret": "${BINANCE_SECRET_KEY}",
        "ccxt_config": {},
        "ccxt_async_config": {},
        "pair_whitelist": [
            "ETH/USDT:USDT"
        ],
        "pair_blacklist": [
            ".*/_USDT",
            ".*/_BTC",
            ".*/_EUR"
        ]
    },
    "pairlists": [
        {
            "method": "StaticPairList",
            "number_assets": 20,
            "market": "futures",
            "pair_whitelist": [
                "ETH/USDT:USDT"
            ]
        }
    ],
    "edge": {
        "enabled": false,
        "config": {
            "allowed_capital_percentage": 100,
            "capital_available_percentage": 30,
            "max_accepted_diff_percentage": 0.02,
            "process_throttle_secs": 60,
            "min_acceptable_diff": 0.001,
            "pair_whitelist": []
        }
    },
    "telegram": {
        "enabled": false,
        "token": "${TELEGRAM_BOT_TOKEN}",
        "chat_id": "${TELEGRAM_CHAT_ID}"
    },
    "api_server": {
        "enabled": true,
        "listen_ip_address": "127.0.0.1",
        "listen_port": 8080,
        "verbosity": "error",
        "enable_openapi": false,
        "jwt_secret_key": "${JWT_SECRET_KEY}",
        "ws_token": "${WS_TOKEN}",
        "username": "${FREQTRADE_API_USER}",
        "password": "${FREQTRADE_API_PASSWORD}"
    },
    "bot_name": "BraleBot",
    "initial_state": "running",
    "force_entry_enable": false,
    "internal": {
        "process_throttle_secs": 5
    },
    "dataformat_ohlcv": {
        "future": 0
    },
    "datadir": "/freqtrade/user_data",
    "user_data_dir": "/freqtrade/user_data",
    "strategy": "BraleSharedStrategy",
    "strategy_path": "/freqtrade/user_data/strategies",
    "freqai": {
        "enabled": false,
        "model_filename": null
    },
    "db_url": "sqlite:///freqtrade/tradesv3.sqlite"
}